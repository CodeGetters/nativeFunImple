{
  "name": "nativeFunImple",
  "version": "1.0.0",
  "description": "Native function implementation",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "lint-staged": "npx lint-staged",
    "prepare": "husky",
    "format": "prettier --write ./**/*.{vue,ts,tsx,js,jsx,css,less,scss,json,md}"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
