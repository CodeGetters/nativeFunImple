{
  // "rootMode": "upward",
  "presets": [
    [
      "@babel/preset-typescript",
      {
        "isTSX": false,
        "allExtensions": false,
        "allowNamespaces": true,
        "onlyRemoveTypeImports": false
      }
    ],
    [
      "@babel/env",
      {
        "modules": "commonjs",
        "useBuiltIns": "usage",
        "corejs": "3.6.5",
        "targets": {
          "edge": "17",
          "firefox": "60",
          "chrome": "67",
          "safari": "11.1"
        }
      }
    ]
  ]
}
